{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "record",
  "type": "object",
  "properties": {
    "recordId": {
      "type": "string"
    },
    "entryDate": {
      "type": "string"
    },
    "notes": {
      "type": "string"
    },
    "recordType": {
      "type": "string"
    },
    "attachments": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "attachmentId": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "lastModifiedDate": {
            "type": "string"
          },
          "size": {
            "type": "number"
          },
          "type": {
            "type": "string"
          }
        }
      }
    }
  },
  "required": [
    "entryDate"
  ],
  "additionalProperties": false
}
